{
  "name": "pixelize",
  "version": "1.0.0",
  "description": "pixelizes an image",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "all": "npm run build && npm run start",
    "build": "esbuild ./src/index.ts --platform=node --outdir=dist",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "keywords": [
    "image",
    "quantization",
    "pixel"
  ],
  "author": "josh oertel",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.16",
    "@types/sharp": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "esbuild": "^0.14.25",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.0",
    "sharp": "^0.30.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ],
    "*.json": "prettier --write"
  }
}
